<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kyrsavayajava.EditDiagnostics">
    <children>
        <Label fx:id="labelName" layoutX="63.0" layoutY="20.0"/>
        <Label fx:id="labelPhone" layoutX="63.0" layoutY="40.0"/>
        <Label fx:id="labelReason" layoutX="63.0" layoutY="60.0"/>
        <Label fx:id="labelStatus" layoutX="63.0" layoutY="80.0"/>
        <Button fx:id="setMasterButton" layoutX="63.0" layoutY="120.0" mnemonicParsing="false"
                onAction="#setMasterToRequest" prefHeight="81.0" prefWidth="158.0" text="Назначить приемщика"/>
        <Button fx:id="startDiagnosticsButton" layoutX="63.0" layoutY="200.0" mnemonicParsing="false"
                onAction="#startDiagnostics" prefHeight="81.0" prefWidth="157.0" text="Провести диагностику"/>
    </children>
</AnchorPane>
