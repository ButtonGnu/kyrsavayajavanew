<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Label?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kyrsavayajava.EditJob">
   <children>
      <Label fx:id="labelName" layoutX="63.0" layoutY="20.0"/>
      <Label fx:id="labelPhone" layoutX="63.0" layoutY="40.0"/>
      <Label fx:id="labelReason" layoutX="63.0" layoutY="60.0"/>
      <Label fx:id="labelStatus" layoutX="63.0" layoutY="80.0"/>
      <Button fx:id="assignToButton" layoutX="63.0" layoutY="120.0" mnemonicParsing="false" onAction="#assignTo" prefHeight="55.0" prefWidth="171.0" text="Назначить автослесаря" />
      <Button fx:id="startJobButton" layoutX="63.0" layoutY="200.0" mnemonicParsing="false" onAction="#startJob" prefHeight="55.0" prefWidth="171.0" text="Провести работу" />
      <Button fx:id="cancelButton" layoutX="63.0" layoutY="300.0" mnemonicParsing="false" onAction="#cancel" prefHeight="55.0" prefWidth="171.0" text="Отказаться от работы" />
   </children>
</AnchorPane>
